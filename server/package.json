{
  "author": "Adam Nguyen",
  "name": "server",
  "license": "MIT",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "dev": "nodemon",
    "prod": "NODE_ENV=production npm run build:start",
    "build": "rm -rf dist/ && tsc -p tsconfig.json && webpack --config webpack.config.js",
    "build:start": "rm -rf dist/ && tsc -p tsconfig.json && webpack --config webpack.config.js && node -r dotenv/config dist/bundle.js",
    "lint:no-fix": "eslint src/**/*.ts",
    "lint": "eslint src/**/*.ts --fix",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@koa/bodyparser": "^5.0.0",
    "boardgame.io": "0.49.8",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "node-persist": "^4.0.1"
  },
  "devDependencies": {
    "@flydotio/dockerfile": "^0.5.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/helmet": "^4.0.0",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "chalk": "4.1.2",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "engines": {
    "npm": ">=9.5.1",
    "node": ">=18.16.0"
  }
}
